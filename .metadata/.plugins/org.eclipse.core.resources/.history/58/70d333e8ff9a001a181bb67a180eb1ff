package cmd;
/*
 * *
 * la commande QUIT stoppe le programme avec la fonction STOP().
 */
public class CommandQuit implements Command {
	
	DrawingTUI a ;
	
	CommandQuit(DrawingTUI a){
		this.a=a ;
	}
	
	@Override
	public void execute() {
		// TODO Auto-generated method stub
		a.stop() ;// a implementer dans DrawingUIT();
		
	}

}
